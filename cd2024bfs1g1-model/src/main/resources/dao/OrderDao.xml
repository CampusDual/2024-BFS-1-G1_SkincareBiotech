<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup xmlns="http://www.ontimize.com/schema/jdbc"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
                 catalog="" schema="${mainschema}" table="ORDERS" datasource="mainDataSource"
                 sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>ORD_ID</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>ORD_ID</Column>
    </UpdateKeys>
    <GeneratedKey>ord_id</GeneratedKey>
    <Queries>
        <Query id="default">
            <AmbiguousColumns>
                <AmbiguousColumn name="PRO_ID" prefix="P"/>
            </AmbiguousColumns>
            <Sentence>
                <![CDATA[
               SELECT
                  #COLUMNS#
               FROM
                  ${mainschema}.ORDERS AS O
                  INNER JOIN ${mainschema}.PRODUCTS AS P ON O.PRO_ID = P.PRO_ID
               #WHERE#
               #ORDER#
            ]]>
            </Sentence>
        </Query>
    </Queries>
</JdbcEntitySetup>